{
    "name": "@donatas123/main",
    "version": "1.0.206",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "pub:removeOldDist": "rm -rf dist",
        "pub:createDistDir": "cp src dist -r",
        "pub:deleteTSFiles": "find dist -type f -name '*.ts' ! -name '*.d.ts' -delete",
        "pub:updateVersion": "npm version patch --force",
        "pub:publishOnlyDist": "cp package.json dist/package.json && (cd dist && npm publish) && rm dist/package.json",
        "pub": "npm run test && npm run pub:removeOldDist && npm run pub:createDistDir && npm run build && npm run pub:deleteTSFiles && npm run pub:updateVersion && npm run push-to-github && npm run pub:publishOnlyDist && npm run pub:removeOldDist",
        "test": "tsc jest.config.ts && jest",
        "push-to-github": "git add . && git commit --allow-empty -m \"publish\" && git push"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/cookie": "^0.5.1",
        "@types/js-cookie": "^3.0.2",
        "atob": "^2.1.2",
        "cookie": "^0.4.1",
        "cross-fetch": "^3.1.4",
        "date-fns": "^2.22.1",
        "js-cookie": "^2.2.1",
        "jsonwebtoken": "^8.5.1",
        "leaflet": "^1.8.0",
        "vue": "^3.2.37"
    },
    "jest": {
        "transform": {
            "^.+\\.js?$": "babel-jest"
        }
    },
    "devDependencies": {
        "@types/atob": "^2.1.2",
        "@types/jest": "^26.0.23",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/ws": "^7.4.4",
        "babel-jest": "^27.0.2",
        "copyfiles": "^2.4.1",
        "force-file-structure": "^1.0.1",
        "jest": "^27.0.4",
        "ts-jest": "^27.0.2"
    }
}
